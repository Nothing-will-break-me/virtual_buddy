    "version": "0.2.0",
    "configurations": [
        {
            "name": "Client Windows",
            "cwd": "frontend",
            "request": "launch",
            "type": "dart",
            "args": [
                "-d", "windows"
            ]
        },
        {
            "name": "Client Web",
            "cwd": "frontend",
            "request": "launch",
            "type": "dart",
            "args": [
                "--web-renderer",
                "canvaskit",
                "--web-port",
                "42765",
                "-d",
                "chrome"
            ]     
        },
    ]
}